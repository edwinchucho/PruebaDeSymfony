{% extends 'base.html.twig' %}
{% block title %}editCategory{% endblock %}
{% block stylesheets %}
    {{ parent() }}
    <link rel="stylesheet" href="{{ asset('css/centrar.css')}}">
{% endblock %}
{% block body %}
<div class="form-signin">
    {{ form_start(form) }}
    {{ form_widget(form) }}
    <button class="btn btn-success">{{ button_label|default('guardar') }}</button>
    <br>
    <br>
    <a class="btn btn-primary" href="{{ path('app_category') }}">Regresar</a>
    {{ form_end(form) }}
</div>
{% endblock %}



